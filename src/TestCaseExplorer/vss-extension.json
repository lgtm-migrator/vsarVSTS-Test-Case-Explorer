{
  "manifestVersion": 1,
  "id": "almrangers-vsoextensions-testcases",
  "version": "1.1.0",
  "name": "Test Case Explorer Extension",
  "description": "An extension to explore test cases.",
  "publisher": "mathias",
  "public": false,
  "scopes": [
    "vso.work_write",
    "vso.test_write",
    "vso.profile"
  ],
   "icons": {
    "default": "img/Short-Icon.png"
  },
  "categories": [
    "Testing"
  ],
  "tags": [
    "test",
    "test planning",
    "test case management"
  ],
  "screenshots": [
    {
      "path": "content/screen1.png"
    },
    {
      "path": "content/screen2.png"
    }
  ],
  "content": {
    "details": {
      "path": "content/overview.md"
    },
    "license": {
      "path": "content/MIT-1.txt"
    }
  },
   "links": {
    "home": { "uri": "http://aka.ms/vsoext13" },
    "getstarted": { "uri": "http://aka.ms/vsoext13" },
    "support": { "uri": "http://aka.ms/vsarcontact" },
    "repository": { "uri": "http://aka.ms/vsoext13" },
    "issues": { "uri": "http://aka.ms/vsoext13" },
    "learn": {"uri": "http://aka.ms/vsoext13"}
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
    "files": [
    {
      "path": "index.html",
      "addressable": true
    },
    {
      "path": "scripts",
      "addressable": true
    },
    {
      "path": "sdk",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    },
    {
      "path": "css",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "test-case-hub",
      "type": "ms.vss-web.hub",
      "description": "Test Case Explorer",
      "targets": [
        "ms.vss-test-web.test-hub-group"
      ],
      "properties": {
        "name": "Test cases",
        "order": 100,
        "uri": "index.html"
      }
    }
  ]
}
