{
    "manifestVersion": 1,
    "id": "almrangers-vsoextensions-testcases-dev",
    "version": "0.2.0",
    "name": "Test Case Explorer Extension",
    "description": "An extension to manage test cases.",
    "baseUri": "https://localhost:44300/",
    "publisher": "mathias",
    "scopes": [
        "vso.work_write",
        "vso.test_write",
        "vso.profile"
      ],
    "icons": {
      "default": "img/Short_Icon.jpg",
      "wide":  "img/Wide_Icon.jpg"
    },
  "tags": [
    "Sample"
  ],
  "galleryFlags": [
      
    ],
    "categories": [
        "Productivity"
    ],
    "contributions": [
        {
            "id": "test-case-hub",
            "type": "ms.vss-web.hub",
            "description": "Test Case Explorer",
            "targets": [
                "ms.vss-test-web.test-hub-group"
            ],
            "properties": {
                "name": "Test cases",
                "order": 40,
                "uri": "index.html"
            }
        }
    ]
}
