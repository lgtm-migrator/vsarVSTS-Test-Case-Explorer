{
  "manifestVersion": 1,
  "id": "almrangers-vsoextensions-testcases-dev",
  "version": "1.1.0",
  "name": "Test Case Explorer Extension (dev)",
  "description": "An extension to explore test cases.",
  "publisher": "mathias",
  "scopes": [
    "vso.work_write",
    "vso.test_write",
    "vso.profile"
  ],
  "icons": {
    "default": "img/Short-Icon.jpg",
    "wide": "img/Wide-Icon.jpg"
  },
  "categories": [
    "Testing"
  ],
  "tags": [
    "test",
    "test planning",
    "test case management"
  ],
  "screenshots": [
    {
      "path": "content/screen1.png"
    },
    {
      "path": "content/screen2.png"
    }
  ],
  "content": {
    "details": {
      "path": "content/overview.md"
    },
    "license": {
      "path": "content/MIT-1.txt"
    }
  },
  "links": {
    "home": "http://aka.ms/vsoext13",
    "getstarted": "http://aka.ms/vsoext13",
    "support": "http://aka.ms/vsarcontact",
    "repository": "http://aka.ms/vsoext13",
    "issues": "http://aka.ms/vsoext13"
  },
    "files": [
    {
      "path": "index.html",
      "addressable": true
    },
    {
      "path": "scripts",
      "addressable": true
    },
    {
      "path": "sdk",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    },
    {
      "path": "css",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "test-case-hub",
      "type": "ms.vss-web.hub",
      "description": "Test Case Explorer",
      "targets": [
        "ms.vss-test-web.test-hub-group"
      ],
      "properties": {
        "name": "Test cases",
        "order": 100,
        "uri": "index.html"
      }
    }
  ]
}
