{
    "manifestVersion": 1,
    "id": "almrangers-vsoextensions-testcases",
    "version": "1.0.0",
    "name": "Test Case Explorer Extension",
    "description": "An extension to explore test cases.",
    "baseUri": "https://test-textcaseexplorer-vsoextensions-almrangers.azurewebsites.net/", 
    "publisher": "mathias", 
    "scopes": [
        "vso.work_write",
        "vso.test_write",
        "vso.profile"
      ],
    "icons": {
      "default": "img/Short_Icon.jpg",
      "wide":  "img/Wide_Icon.jpg"
    },
  "galleryFlags": [
    ],
    "categories": [
        "Productivity"
    ],
    "contributions": [
        {
            "id": "test-case-hub",
            "type": "ms.vss-web.hub",
            "description": "Test Case Explorer",
            "targets": [
                "ms.vss-test-web.test-hub-group"
            ],
            "properties": {
                "name": "Test cases",
                "order": 100,
                "uri": "index.html"
            }
        }
    ]
}
