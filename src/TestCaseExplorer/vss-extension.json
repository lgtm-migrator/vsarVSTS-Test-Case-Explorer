{
  "manifestVersion": 1,
  "id": "DEV-TestCaseExplorer",
  "version": "0.0.1",
  "name": "DEV://Test Case Explorer",
  "description": "Manage your test cases better. Find, filter, analyze usage of test cases, and more.",
  "publisher": "mskold",

  "scopes": [
    "vso.work_write",
    "vso.test_write",
    "vso.profile"
  ],
  "icons": {
    "default": "img/Short-Icon.png"
  },
  "categories": [
    "Test"
  ],
  "tags": [
    "test",
    "test planning",
    "test case management"
  ],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "mp_terms.md"
    }
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "links": {
    "support": { "uri": "http://aka.ms/vsarcontact" }
  },
  "branding": {
    "color": "rgb(51,65,133)",
    "theme": "dark"
  },
    "files": [
    {
      "path": "index.html",
      "addressable": true
    },
    {
      "path": "scripts",
      "addressable": true
    },
    {
      "path": "sdk",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    },
    {
      "path": "css",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "test-case-hub",
      "type": "ms.vss-web.hub",
      "description": "Test Case Explorer",
      "targets": [
        "ms.vss-test-web.test-hub-group"
      ],
      "properties": {
        "name": "Test cases",
        "order": 100,
        "uri": "index.html"
      }
    }
  ]
}
